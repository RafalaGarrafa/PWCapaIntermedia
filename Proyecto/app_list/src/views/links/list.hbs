  
<div class="container p-4">
    <div class="row">
        {{#each links}}
            <div class="col-md-3">
                <div class="card text-center">
                    <div class="card-body">
                        <form action="/links/go/{{id}}" method="POST">

                        <button class="listBack">
                            <h3 class="card-title text-uppercase" id="listName">{{listname}}</h3>
                            <!--<a href="http://localhost:4200/games/{{id}}" target="_blank">

                                <h3 class="card-title text-uppercase">
                                    {{listname}}
                                </h3>
                            </a>-->
                            <p class="m-2">{{resumen}}</p>
                            <p>{{timeago created}}</p>
                            <a href="/links/delete/{{id}}" class="btn btn-danger deleteBoton"> Delete list</a>
                            <a href="/links/edit/{{id}}" class="btn btn-primary editeBoton"> Edit</a>
                        </button>

                        </form>
                    </div>
                </div>
            </div>
            {{else}}
                <div class="col-md-4 mx-auto">
                    <div class="card card-body text-center">
                        <p>Chale... No tienes nada guardado :(</p>
                        <a href="/links/add">Crea una :v </a>
                    </div>
                </div>
        {{/each}}
    </div>
</div>